

[gas]
    gamma   1.4
    R       0.71428571428
end


[bc]
    name    Farfield
    type    farfield
    T       1
    mach    0.8
    angle   0.0
    p       1
end


[bc]
    name    Airfoil
    type    slip-wall
end



[solver]
    type                implicit
    second_order        true
    viscosity_model     inviscid
    tolerance           1e-8
    start_cfl           5
    slope_cfl           1
    max_cfl             100
    relaxation          0.8
    gradients           least-squares
    limiter_k           5.
    rhs_iterations      5
    airfoil_name        Airfoil
end


